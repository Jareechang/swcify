{
  "name": "@shopify/swcify",
  "version": "0.1.8",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@napi-rs/cli": "^1.0.0",
    "jest": "^27.1.0"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "swcify.node",
    "native"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "swcify",
    "triples": {
      "defaults": true
    }
  },
  "scripts": {
    "test": "yarn jest",
    "prepublishOnly": "yarn build:test",
    "build": "napi build --platform --release native",
    "build:test": "rustup target add x86_64-apple-darwin && DEBUG=\"napi:*\" napi build --platform native",
    "build:debug": "napi build --platform native"
  },
  "dependencies": {
    "@node-rs/helper": "^1.2.1"
  }
}
